@model IEnumerable<DaVinci.Models.Feedbacks>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Comentario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataFeedback)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Avaliacao)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Comentario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataFeedback)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Avaliacao)
            </td>

            <td>
                <a asp-action="Edit" asp-route-id="@item.IdFeedback">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdFeedback">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdFeedback">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
